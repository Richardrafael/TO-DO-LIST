{
    "_version": "1.61.0",
    "sap.app": {
        "id": "sap.ui.demo.todo",
        "type": "application",
        "title": "{{appTitle}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
    "sap.ui5": {
        "dependencies": {
			"minUI5Version": "1.98.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {}
            }
        },
        "rootView": {
            "viewName": "sap.ui.demo.todo.view.App",
            "type": "XML",
            "id": "app"
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.ui.demo.todo.i18n.i18n",
                    "async": true
                }
            },
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/todoitems.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/styles.css"
                }
            ]
        }
    }
}